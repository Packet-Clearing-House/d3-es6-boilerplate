{
 'extends': 'airbnb/base',

  'env': {
    'browser': true,
    'node': true
  },

  'globals': {
    'd3': true,
    'ENV' : true
  },

 'rules': {
   'strict': 0,
   'func-names': 0,
   'no-param-reassign': 0,
   'no-loop-func': 0,
   'arrow-body-style': 0,
   'new-cap': 0,
   'no-alert': 0,
   'no-console': 0,
   'object-shorthand' : 0,
   'no-use-before-define': [1, 'nofunc']
 }
}
